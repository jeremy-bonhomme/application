    "hammerjs": "^2.0.8",
    "keycloak-js": "^3.2.1",
    "lodash-es": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.18.1",
    "ng2-dragula": "^1.5.0",
    "rxjs": "^5.4.1",
