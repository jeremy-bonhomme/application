{
  "name": "App",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gulp process && ng build --prod --locale=fr-FR",
    "build-en": "gulp process && ng build --prod --environment=prod-en --locale=en-US --output-path=dist/en --base-href=/en/ --i18n-format=xlf --i18n-file=src/locale/en.xlf",
    "build-es": "gulp process && ng build --prod --environment=prod-es --locale=es-ES --output-path=dist/es --base-href=/es/ --i18n-format=xlf --i18n-file=src/locale/es.xlf",
    "e2e": "ng e2e",
    "i18n": "gulp process && ng xi18n --output-path=src/locale --out-file=fr.xlf",
    "lint": "ng lint",
    "start": "concurrently --raw \"gulp serve\" \"ng serve --hmr\"",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
